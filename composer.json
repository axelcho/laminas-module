{
    "name":        "axelcho/laminas-module",
    "description": "Lamina Module that provides Doctrine basic functionality",
    "type":        "library",
    "license":     "MIT",
    "homepage":    "http://www.axelcho.com/",
    "keywords":    [
        "doctrine",
        "module",
        "laminas"
    ],

    "require": {
        "php":                               "^7.1",
        "doctrine/common":                   "^2.8",
        "doctrine/cache":                    "^1.7",
        "symfony/console":                   "^3.3 || ^4.0 || ^5.0",
        "laminas/laminas-authentication": "^2.5.3",
        "laminas/laminas-cache":          "^2.7.1",
        "laminas/laminas-form":           "^2.11",
        "laminas/laminas-hydrator":       "^2.3",
        "laminas/laminas-mvc":            "^3.1",
        "laminas/laminas-paginator":      "^2.8",
        "laminas/laminas-servicemanager": "^3.3",
        "laminas/laminas-stdlib":         "^3.1",
        "laminas/laminas-validator":      "^2.10"
    },
    "require-dev": {
        "phpunit/phpunit":                  "^7.5.2",
        "predis/predis":                    "^1.1",
        "squizlabs/php_codesniffer":        "^2.7",
        "laminas/laminas-i18n":          "^2.7",
        "laminas/laminas-log":           "^2.9",
        "laminas/laminas-modulemanager": "^2.8",
        "laminas/laminas-mvc-console":   "^1.1.11",
        "laminas/laminas-serializer":    "^2.8",
        "laminas/laminas-session":       "^2.8",
        "laminas/laminas-test":          "^3.1.1",
        "ocramius/package-versions":       "^1.5.1"
    },
    "suggest": {
        "doctrine/data-fixtures":         "Data Fixtures if you want to generate test data or bootstrap data for your deployments",
        "laminas/laminas-mvc-console": "^1.1.11 if you want to use the ZF3 console libraries"
    },
    "autoload": {
        "psr-0": {
            "DoctrineModule\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-0": {
            "DoctrineModuleTest\\": "tests/"
        }
    },
    "bin": [
        "bin/doctrine-module"
    ]
}
